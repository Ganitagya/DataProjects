{
    "apiVersion": "1.2",
    "version": "1.0",
    "name": "Events Calendar",
    "id": "js-dev-starter",
    "icon": "icon.svg",
    "properties": [
        {
            "name": "myProperty",
            "type": "string",
            "defaultValue": "myValue"
        }
    ],
    "dataViewDefinition": {
        "colorAxis": {
            "mode": "dual",
            "dropTarget": {
                "icon": "Color",
                "description": "Color by {0}"
            }
        },
		
		"axes" : [
			{
				"name": "event_name",
				"mode": "categorical"
			},
			{
				"name": "event_startdate",
				"mode": "categorical"
			},
			{
				"name": "event_enddate",
				"mode": "categorical"
			},
			{
				"name": "event_category",
				"mode": "categorical"
			}
		]
       
    },
    "files": [
        "index.html",
        "main.css",
		"calendar.js",
		"bootstrap_icon.css",
		"bootstrap_min.css",
        "main.js"
    ]
}